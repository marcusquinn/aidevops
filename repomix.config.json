{
  "output": {
    "style": "xml",
    "filePath": "repomix-output.xml",
    "removeComments": false,
    "showLineNumbers": true,
    "topFilesLength": 10,
    "headerText": "AI DevOps Framework - Codebase Context\nGenerated by Repomix for AI analysis\nSee: https://github.com/marcusquinn/aidevops"
  },
  "include": [
    "**/*.md",
    "**/*.sh",
    "**/*.json.txt",
    "**/*.yaml",
    "**/*.yml",
    "**/*.toml",
    "setup.sh",
    "VERSION",
    "CHANGELOG.md"
  ],
  "ignore": {
    "useGitignore": true,
    "customPatterns": [
      "repomix-output.*",
      "**/*.log",
      "**/node_modules/**",
      "**/.git/**",
      "**/tmp/**",
      "**/*.backup",
      "**/*.bak"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
